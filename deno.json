{
  "tasks": {
    "dev": "deno run -A --unstable --watch source/server.ts",
    "serve": "deno run --allow-env --allow-read=. --allow-run --allow-net --allow-write=repo --unstable source/server.ts"
  },
  "imports": {
    "std/": "https://deno.land/std@0.181.0/"
  },
  "fmt": {
    "options": {
      "singleQuote": true,
      "semiColons": false
    }
  }
}
